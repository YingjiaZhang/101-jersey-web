<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="school.thoughtworks.pos.bean.CartMapper">
    <resultMap id="categoryMap" type="school.thoughtworks.pos.bean.Cart">
        <id property="id" column="id"></id>
        <result column="name" property="name"></result>
    </resultMap>
    <insert id="insertCart" parameterType="school.thoughtworks.pos.bean.Cart" useGeneratedKeys="true" keyProperty="id">
        insert into cart value(null,#{userId});
    </insert>


</mapper>